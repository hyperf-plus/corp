{
    "name": "hyperf-plus/corp",
    "description": "Hyperf 组织架构包 - 提供企业、部门、员工、角色等基础组织架构管理功能",
    "keywords": [
        "hyperf",
        "corp",
        "organization",
        "department",
        "employee",
        "role"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "HyperfPlus",
            "email": "dev@hyperf-plus.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "hyperf/database": "~3.1.0",
        "hyperf/di": "~3.1.0",
        "hyperf/model-cache": "~3.1.0",
        "hyperf/cache": "~3.1.0",
        "hyperf/context": "~3.1.0",
        "hyperf/config": "~3.1.0",
        "hyperf/framework": "~3.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.5"
    },
    "suggest": {
        "hyperf-plus/scope": "数据作用域隔离包"
    },
    "autoload": {
        "psr-4": {
            "HPlus\\Corp\\": "src/"
        }
    },
    "extra": {
        "hyperf": {
            "config": "HPlus\\Corp\\ConfigProvider"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
