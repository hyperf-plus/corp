{
    "name": "hyperf-plus/corp",
    "description": "Hyperf 组织架构基础包 - 提供企业、部门、员工、角色、权限、数据隔离等完整功能。支持企业级和用户级两种数据隔离方式，内置协作者权限管理，开箱即用。",
    "keywords": [
        "hyperf",
        "php",
        "organization",
        "rbac",
        "permission",
        "data-isolation",
        "collaborator",
        "enterprise",
        "department",
        "employee",
        "role",
        "access-control",
        "multi-tenant",
        "corp"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "HyperfPlus",
            "email": "dev@hyperf-plus.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "hyperf/database": "~3.1.0",
        "hyperf/di": "~3.1.0",
        "hyperf/model-cache": "~3.1.0",
        "hyperf/cache": "~3.1.0",
        "hyperf/context": "~3.1.0",
        "hyperf/config": "~3.1.0",
        "hyperf/framework": "~3.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "HPlus\\Corp\\": "src/"
        }
    },
    "extra": {
        "hyperf": {
            "config": "HPlus\\Corp\\ConfigProvider"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
